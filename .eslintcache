itch, Redirect } from \"react-router-dom\";\n\n// Functions, Components and Pages\nimport { fetchProjectData } from \"./services/RailsApi\";\n\nfunction NotFound() {\n  return (\n    <div>\n      <h1>404 not found</h1>\n    </div>\n  );\n}\n\nfunction App() {\n  const [locationData, setLocationData] = useState([\n    {\n      city: \"\",\n      state: \"\",\n      breweries: [],\n    },\n  ]);\n\n  async function getData() {\n    const data = await fetchProjectData();\n    setLocationData(data);\n  }\n\n  useEffect(() => {\n    getData();\n  }, []);\n\n  return (\n    <div className=\"App\">\n      <Header />\n      <p>Check out local breweries in the following cities.</p>\n\n      <Switch>\n        <Route\n          exact\n          path=\"/\"\n          render={(props) => (\n            <div>\n              {locationData.map((locations, idx) => (\n                <HomeContent key={idx} locations={locations} />\n              ))}\n            </div>\n          )}\n        />\n        <Route\n          exact\n          path=\"/breweries\"\n          render={(props) => <SelectionPage locationData={locationData} />}\n        />\n\n        <Route component={NotFound} />\n      </Switch>\n\n      <Footer />\n    </div>\n  );\n}\n\nexport default App;\n","/Users/Steven/Documents/Coding Stuff/Unit 4/Brew_Advisor-Project 4/brewery_advisor_client/src/components/HomeContent/HomeContent.js",[],"/Users/Steven/Documents/Coding Stuff/Unit 4/Brew_Advisor-Project 4/brewery_advisor_client/src/index.js",[],"/Users/Steven/Documents/Coding Stuff/Unit 4/Brew_Advisor-Project 4/brewery_advisor_client/src/pages/SelectionPage/SelectionPage.js",[],"/Users/Steven/Documents/Coding Stuff/Unit 4/Brew_Advisor-Project 4/brewery_advisor_client/src/components/Header/Header.js",[],{"ruleId":"24","severity":1,"message":"25","line":9,"column":25,"nodeType":"26","messageId":"27","endLine":9,"endColumn":33},"no-unused-vars","'Redirect' is defined but never used.","Identifier","unusedVar"]